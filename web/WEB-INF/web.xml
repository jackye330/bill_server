<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.Servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateUserServlet</servlet-name>
        <servlet-class>com.Servlet.UpdateUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateUserServlet</servlet-name>
        <url-pattern>/updateUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetRecordServlet</servlet-name>
        <servlet-class>com.Servlet.GetRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetRecordServlet</servlet-name>
        <url-pattern>/getRecord</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddRecordServlet</servlet-name>
        <servlet-class>com.Servlet.AddRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddRecordServlet</servlet-name>
        <url-pattern>/addRecord</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteRecordServlet</servlet-name>
        <servlet-class>com.Servlet.DeleteRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteRecordServlet</servlet-name>
        <url-pattern>/deleteRecord</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetPrivateKeyServlet</servlet-name>
        <servlet-class>com.Servlet.GetPrivateKeyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPrivateKeyServlet</servlet-name>
        <url-pattern>/getPrivateKey</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SendVCServlet</servlet-name>
        <servlet-class>com.Servlet.SendVCServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SendVCServlet</servlet-name>
        <url-pattern>/sendVC</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckVCServlet</servlet-name>
        <servlet-class>com.Servlet.CheckVCServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckVCServlet</servlet-name>
        <url-pattern>/checkVC</url-pattern>
    </servlet-mapping>
</web-app>
